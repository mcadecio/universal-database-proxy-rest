<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dercio.database_proxy.repositories.football.NationalFootballTeamsRepository">
    <select id="findTeamByName" resultType="com.dercio.database_proxy.repositories.football.NationalFootballTeam">
        SELECT *
        FROM football.national_football_teams
        WHERE name = #{name}
    </select>
</mapper>